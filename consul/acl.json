{
        "skip_leave_on_interrupt": true
}
{
        "encrypt": "z*"
}
{
        "datacenter": "office-msk",
        "start_join": [
                "*.*.*.*", "*.*.*.*"
        ],
        "retry_interval": "5s",
        "retry_max": 30
}
{
        "acl_datacenter": "office-*",
        "acl_default_policy": "allow",
        "acl_down_policy": "extend-cache",
        "acl_agent_token": "*"
}
{
	"node_name": "agent0",
	"node_meta": {
		"dc": "office-*"
	},
	"raft_protocol": 3,
	"data_dir": "/var/lib/consul",
	"log_level": "WARN"
}
